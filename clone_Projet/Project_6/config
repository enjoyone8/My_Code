[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
[remote "My_Code"]
	url = git@github.com:enjoyone8/My_Code.git
	fetch = +refs/heads/*:refs/remotes/My_Code/*
[branch "master"]
	remote = My_Code
	merge = refs/heads/master
[alias]  
  co = checkout  
  ci = commit  
  st = status  
  br = branch  
  ps = push  
  log = log --graph --
  lg  = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Cres %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --all 
  l  = log --color --graph --pretty
